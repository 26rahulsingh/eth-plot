  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Buy Random Plots",
      "program": "${workspaceRoot}/src/scratch/buy-random-plots.js",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}",
      "preLaunchTask": null,
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
      "runtimeArgs": [
          "--nolazy"
      ],
      "env": {
          "NODE_ENV": "production"
      },
      "sourceMaps": false,
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Current File",
      "program": "${file}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/devServer.js"
    }
  ]
}